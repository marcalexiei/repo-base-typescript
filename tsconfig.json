{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@_models/*": [
        "src/models/*"
      ]
    },
    "module": "ES6",
    "target": "es2016",
    "sourceMap": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "dist"
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules",
    "**/node_modules/*"
  ]
}
